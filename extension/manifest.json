{
  "manifest_version": 3,
  "name": "Qwen Session Saver",
  "version": "1.0",
  "description": "Saves authentication state from chat.qwen.ai to a local server.",
  "permissions": [
    "activeTab",
    "scripting",
    "cookies"
  ],
  "host_permissions": [
    "https://chat.qwen.ai/"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}